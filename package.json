{
  "name": "web-ext-deploy",
  "version": "0.0.1",
  "description": "A tool for deploying WebExtensions to multiple stores.",
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "bin": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/avi12/web-ext-deploy"
  },
  "keywords": [
    "web-ext",
    "web-ext-deploy",
    "web-ext-publish",
    "chrome-deploy",
    "chrome-publish",
    "firefox-deploy",
    "firefox-publish",
    "opera-deploy",
    "opera-publish",
    "edge-deploy",
    "edge-publish"
  ],
  "files": [
    "dist/**/*"
  ],
  "author": {
    "name": "avi12",
    "email": "avi6106@gmail.com",
    "url": "https://avi12.com"
  },
  "bugs": {
    "email": "avi6106@gmail.com",
    "url": "https://github.com/avi12/web-ext-deploy/issues"
  },
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/avi12/web-ext-deploy#readme",
  "scripts": {
    "debug:dev": "nodemon --config nodemon-prod.json",
    "debug:prod": "nodemon --",
    "clean": "rimraf dist esm",
    "prepare": "pnpm run clean && pnpm run build",
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "build:cjs": "ncc build src/index.ts -o dist -m",
    "build:esm": "tsc --target ESNext --module ES6 --outDir esm",
    "watch": "tsc --watch",
    "types:check": "tsc --noEmit",
    "format": "prettier --write \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,test,examples}/**/*.{ts,tsx}\" --fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run types:check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pnpm run lint:fix",
      "pnpm run format",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/puppeteer": "^5.4.3",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "@zeit/ncc": "^0.22.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "camel-case": "^4.1.2",
    "dotenv": "^8.2.0",
    "prompt-promise": "^1.0.3",
    "puppeteer": "5",
    "puppeteer-extra": "^3.1.17",
    "puppeteer-extra-plugin-recaptcha": "^3.3.3",
    "yargs": "^16.2.0"
  }
}

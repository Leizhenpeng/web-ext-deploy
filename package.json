{
  "name": "web-ext-deploy",
  "version": "0.0.1",
  "description": "A tool for deploying WebExtensions to multiple stores.",
  "main": "./dist/index.js",
  "module": "./dist-esm/index.js",
  "bin": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/avi12/web-ext-deploy"
  },
  "keywords": [
    "web-ext",
    "web-ext-deploy",
    "web-ext-publish",
    "chrome-deploy",
    "chrome-publish",
    "firefox-deploy",
    "firefox-publish",
    "opera-deploy",
    "opera-publish",
    "edge-deploy",
    "edge-publish"
  ],
  "files": [
    "dist/**/*"
  ],
  "author": {
    "name": "avi12",
    "email": "avi6106@gmail.com",
    "url": "https://avi12.com"
  },
  "bugs": {
    "email": "avi6106@gmail.com",
    "url": "https://github.com/avi12/web-ext-deploy/issues"
  },
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/avi12/web-ext-deploy#readme",
  "scripts": {
    "debug:dev": "nodemon --config nodemon-dev.json",
    "debug:prod": "nodemon --",
    "clean": "rimraf dist dist-esm",
    "prepare": "pnpm run clean && pnpm run build",
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "build:cjs": "tsc --build",
    "build:esm": "tsc --target ESNext --module ES6 --outDir dist-esm",
    "watch": "tsc --watch",
    "types:check": "tsc --noEmit",
    "format": "prettier --write \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,test,examples}/**/*.{ts,tsx}\" --fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run types:check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pnpm run lint:fix",
      "pnpm run format",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/async-lock": "^1.1.2",
    "@types/inquirer": "^7.3.1",
    "@types/puppeteer": "^5.4.3",
    "@types/user-agents": "^1.0.0",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "camel-case": "^4.1.2",
    "chrome-webstore-upload": "^0.4.4",
    "compare-versions": "^3.6.0",
    "dotenv": "^8.2.0",
    "inquirer": "^8.0.0",
    "mitt": "^2.1.0",
    "parse-duration": "^0.4.4",
    "puppeteer": "5",
    "puppeteer-extra": "^3.1.17",
    "puppeteer-extra-plugin-anonymize-ua": "^2.2.16",
    "puppeteer-extra-plugin-minmax": "^1.1.2",
    "puppeteer-extra-plugin-stealth": "^2.7.5",
    "user-agents": "^1.0.574",
    "yargs": "^16.2.0",
    "zip-local": "^0.3.4"
  }
}
